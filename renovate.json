{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "encrypted": {
    "npmrc": ""
  },
  "packageRules": [
    {
      "packagePatterns": ["*"],
      "excludePackagePatterns": ["@privy-io"],
      "enabled": false
    },
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    }
  ],
  "rangeStrategy": "pin",
  "constraints": {
    "node": ">=16.0.0 <17.0.0"
  }
}
